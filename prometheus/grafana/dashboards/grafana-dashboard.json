{
  "id": null,
  "title": "Tabario Video Generation",
    "tags": ["video-generation", "ai", "comfyui"],
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 16,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Job Throughput",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(jobs_enqueued_total[5m])",
            "refId": "A",
            "interval": "",
            "legendFormat": "Jobs Enqueued (rate)"
          },
          {
            "expr": "rate(jobs_completed_total[5m])",
            "refId": "B",
            "interval": "",
            "legendFormat": "Jobs Completed (rate)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "displayMode": "lcd",
              "orientation": "horizontal"
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Job Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(jobs_completed_total[5m]) / rate(jobs_enqueued_total[5m]) * 100",
            "refId": "A",
            "interval": "",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "continuous-GrYlRd"
            },
            "custom": {
              "displayMode": "lcd"
            },
            "mappings": [],
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "#EAB839",
                  "value": 80
                },
                {
                  "color": "green",
                  "value": 95
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        }
      },
